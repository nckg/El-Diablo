<div class="row-fluid" id="hero-overview">
	{{#heroes}}
	<div class="span3 hero {{class}}">
		<a href="#/hero/{{id}}" data-id="{{id}}">
			<h4>{{name}}</h4>
			<span class="{{class}} level">{{level}}</span>
			<p class="classname">{{class}}{{{getParagonLevel}}}</p>
		</a>
	</div>
	{{/heroes}}
</div>